{
    "name": "koalafacade/diamond-console",
    "description": "Laravel Package for generate DDD structure easily for your projects",
    "keywords": [
        "koala facade",
        "koalafacade",
        "ddd",
        "laravel",
        "laravelpackage",
        "laravel ddd"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KoalaFacade\\DiamondConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stiven Trizky Katuuk",
            "email": "pemudakoding@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Kevin Abrar Khansa",
            "email": "kevariable@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Holiq Ibrahim",
            "email": "holiq.ibrahim376@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.41",
        "illuminate/console": "^9.41",
        "illuminate/filesystem": "^9.41"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "orchestra/testbench": "^7.14"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KoalaFacade\\DiamondConsole\\DiamondConsoleServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
